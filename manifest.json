{
  "manifest_version": 3,
  "name": "boost-tiktok",
  "version": "0",
  "description": "Boost and search for tiktok.com",
  "icons": {
    "128": "128.png"
  },
  "omnibox": {
    "keyword": "tk"
  },
  "background": {
    "service_worker": "bg.js"
  },
  "content_scripts": [{
    "js": ["main.js"],
    "matches": ["https://www.tiktok.com/*"],
    "world": "MAIN",
    "run_at": "document_start",
    "all_frames": true
  }]
}